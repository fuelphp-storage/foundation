{
	"name": "fuelphp/foundation",
	"description": "FuelPHP Framework foundation",
	"homepage": "https://github.com/fuelphp/foundation",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.4",
		"filp/whoops": "dev-master",
		"monolog/monolog": "1.6.0",
		"league/event": "~2.0",
		"fuelphp/dependency": "~2.0",
		"fuelphp/agent": "~2.0",
		"fuelphp/alias": "~2.0",
		"fuelphp/common": "~2.0",
		"fuelphp/display": "~2.0",
		"fuelphp/config": "~2.0",
		"fuelphp/upload": "~2.0",
		"fuelphp/session": "~2.0",
		"fuelphp/database": "~2.0",
		"fuelphp/security": "~2.0",
		"fuelphp/routing": "~2.0",
		"fuelphp/validation": "~2.0",
		"fuelphp/fieldset": "~2.0",
		"fuelphp/orm": "~2.0",
		"fuelphp/leagacy": "~2.0"
	},
	"require-dev": {
		"codeception/codeception": "~2.0"
	},
	"suggest": {
		"michelf/php-markdown": "Required if you want to use the Markdown facade class"
	},
	"autoload": {
		"psr-4": {
			"Fuel\\Foundation\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"minimum-stability": "dev"
}
