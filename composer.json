{
	"name": "fuelphp/foundation",
	"description": "FuelPHP Framework foundation",
	"homepage": "https://github.com/fuelphp/foundation",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=7.0.0",
		"fuelphp/dependency": "dev-master",
		"league/event": "^2.1",
		"fuelphp/config": "dev-master"
	},
	"require-dev": {
		"codeception/codeception": "^2.1",
		"codeception/mockery-module": "^0.2.2",
		"scrutinizer/ocular": "^1.3",
		"phpunit/phpunit": "5.3.*"
	},
	"autoload": {
		"psr-4": {
			"Fuel\\Foundation\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Basic\\": "tests/_support/components/Basic/",
			"Broken\\": "tests/_support/components/Broken/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"minimum-stability": "dev"
}
